	year = {2024},
	note = {Number: 4
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {pH, \textit{ρ}CO$_{\textrm{2}}$, biodeposition, clam, clearance rate, shell strength, thermal stress},
	pages = {673},
	file = {Steeves et al. - 2024 - Effects of Ocean Acidification and Summer Thermal .pdf:C\:\\Users\\brandon.beltz\\Zotero\\storage\\F4XSRM9L\\Steeves et al. - 2024 - Effects of Ocean Acidification and Summer Thermal .pdf:application/pdf},
