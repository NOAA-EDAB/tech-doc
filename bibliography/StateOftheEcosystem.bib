	year = {2013},
	pages = {803--816},
	file = {Full Text PDF:C\:\\Users\\brandon.beltz\\Zotero\\storage\\V9LSLIS2\\Greene et al. - 2013 - Remote climate forcing of decadal-scale regime shi.pdf:application/pdf;Snapshot:C\:\\Users\\brandon.beltz\\Zotero\\storage\\3UFWZQV3\\lo.2013.58.3.html:text/html},
